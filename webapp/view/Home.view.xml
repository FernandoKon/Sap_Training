<mvc:View controllerName="com.lab2dev.traningproject.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page showHeader="false" id="pageHome" title="{i18n>title}">
        <content>
            <List
                id="idProductsList"
                items="{
                    path: '/Products'
                    }">
                <StandardListItem
                    title="{ProductName}"
                    counter="{ProductID}"
                />
            </List>
        </content>
    </Page>
</mvc:View>
